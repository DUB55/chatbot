   {
     "version": 2,
     "builds": [
       {
         "src": "dir/coder.py",
         "use": "@vercel/python"
       },
       {
         "src": "chatbot.html",
         "use": "@vercel/static"
       }
     ],
     "routes": [
       {
         "src": "/api/chatbot",
         "dest": "dir/coder.py"
       },
       {
         "src": "/chatbot.html",
         "dest": "chatbot.html"
       }
     ]
   }
