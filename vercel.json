{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/health",
      "dest": "/api/simple.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/simple.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
