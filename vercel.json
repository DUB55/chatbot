{
  "version": 2,
  "builds": [
    {
      "src": "dir/coder.py",
      "use": "@vercel/python"
    },
    {
      "src": "dir/image.py",
      "use": "@vercel/python"
    },
    {
      "src": "chatbot.html",
      "use": "@vercel/static"
    },
    {
      "src": "image.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/chatbot",
      "dest": "dir/coder.py"
    },
    {
      "src": "/api/image",
      "dest": "dir/image.py"
    },
    {
      "src": "/chatbot.html",
      "dest": "chatbot.html"
    },
    {
      "src": "/image.html",
      "dest": "image.html"
    }
  ],
  "functions": {
    "dir/coder.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}
