{
  "routes": [
    {
      "src": "/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/chatbot",
      "dest": "/api/chatbot.py"
    },
    {
      "src": "/health",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/chatbot.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
