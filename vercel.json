{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/chatbot",
      "destination": "/api/index.py"
    },
    {
      "source": "/chatbot",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/library/list",
      "destination": "/api/index.py"
    },
    {
      "source": "/favicon.ico",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/ping",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/",
      "destination": "/chatbot.html"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}
